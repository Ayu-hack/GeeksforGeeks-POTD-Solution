class Solution {
  public:
    // Function to check if two linked lists are identical
    bool areIdentical(struct Node *head1, struct Node *head2) {
        // code here
       struct Node* temp1=head1;
       struct Node* temp2=head2;
       while(temp1 && temp2){
           if(temp1->data!=temp2->data){
               return 0;
           }
           temp1=temp1->next;
           temp2=temp2->next;
       }
       if(temp2 ||temp1){
           return 0;
       }
        return 1;
    }
};
