class Solution {
  public:
    int getSingle(vector<int>& arr) {
        // code here
        auto first = arr.begin();
        auto last = arr.end();
      
        // calling the sort for the above defined range
        sort(first, last);
        
        for(int i=1; i<arr.size(); i=i+2) {
            if(arr[i]!=arr[i-1]) {
                return arr[i-1];
            }
        }
        
        return arr[arr.size()-1];

    }
};
